<!-- Deferred binding rules for browser selection.                          -->
<!--                                                                        -->
<!-- This module is typically inherited via com.google.gwt.user.User        -->
<!--                                                                        -->
<module>
  <inherits name="com.google.gwt.core.Core"/>
  <inherits name="jsinterop.base.Base"/>
  <inherits name="elemental2.dom.Dom"/>
  <inherits name="org.gwtproject.canvas.dom.DOM"/>
  <inherits name="org.gwtproject.media.dom.DOM"/>
  <inherits name="com.google.gwt.safehtml.SafeHtml"/>
  <inherits name="com.google.gwt.useragent.UserAgent"/>
  <source path="client" />
  <public path="public" />

  <replace-with class="org.gwtproject.dom.client.DOMImplWebkit">
    <when-type-is class="org.gwtproject.dom.client.DOMImpl"/>
    <when-property-is name="user.agent" value="safari"/>
  </replace-with>

</module>
